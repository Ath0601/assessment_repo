<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="xarm5_gripper">

  <!--GRIPPER MACRO-->
  <xacro:macro name="xarm5_gripper" params="prefix parent">

    <joint name="${prefix}gripper_mount_joint" type="fixed">
      <parent link="${parent}"/>
      <child link="${prefix}xarm_gripper_base_link"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <!--BASE LINK-->
    <link name="${prefix}xarm_gripper_base_link">
      <inertial>
        <origin xyz="-0.00065489 -0.0018497 0.048028" rpy="0 0 0"/>
        <mass value="0.54156"/>
        <inertia
          ixx="0.00047106" ixy="3.9292E-07" ixz="2.6537E-06"
          iyy="0.00033072" iyz="-1.0975E-05" izz="0.00025642"/>
      </inertial>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/base_link.stl"/>
        </geometry>
        <material name="White"/>
      </visual>

      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/base_link.stl"/>
        </geometry>
      </collision>
    </link>

    <!--LEFT OUTER KNUCKLE-->
    <link name="${prefix}left_outer_knuckle">
      <inertial>
        <origin xyz="2.9948E-14 0.021559 0.015181" rpy="0 0 0"/>
        <mass value="0.033618"/>
        <inertia
          ixx="1.9111E-05" ixy="-1.8803E-17" ixz="-1.1002E-17"
          iyy="6.6256E-06" iyz="-7.3008E-06" izz="1.3185E-05"/>
      </inertial>

      <visual>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/left_outer_knuckle.stl"/>
        </geometry>
      </visual>

      <collision>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/left_outer_knuckle.stl"/>
        </geometry>
      </collision>
    </link>

    <joint name="${prefix}drive_joint" type="revolute">
      <origin xyz="0 0.035 0.059098" rpy="0 0 0"/>
      <parent link="${prefix}xarm_gripper_base_link"/>
      <child link="${prefix}left_outer_knuckle"/>
      <axis xyz="1 0 0"/>
      <limit lower="0" upper="0.85" effort="50" velocity="2"/>
    </joint>

    <!--LEFT FINGER-->
    <link name="${prefix}left_finger">
      <inertial>
        <origin xyz="-2.4536E-14 -0.016413 0.029258" rpy="0 0 0"/>
        <mass value="0.048304"/>
        <inertia
          ixx="1.7493E-05" ixy="-4.2156E-19" ixz="6.9164E-18"
          iyy="1.7225E-05" iyz="4.6433E-06" izz="5.1466E-06"/>
      </inertial>

      <visual>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/left_finger.stl"/>
        </geometry>
      </visual>

      <collision>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/left_finger.stl"/>
        </geometry>
      </collision>
    </link>

    <joint name="${prefix}left_finger_joint" type="revolute">
      <origin xyz="0 0.035465 0.042039" rpy="0 0 0"/>
      <parent link="${prefix}left_outer_knuckle"/>
      <child link="${prefix}left_finger"/>
      <axis xyz="-1 0 0"/>
      <limit lower="0" upper="0.85" effort="50" velocity="2"/>
    </joint>

    <!--LEFT INNER KNUCKLE-->
    <link name="${prefix}left_inner_knuckle">
      <inertial>
        <origin xyz="1.866E-06 0.0220468 0.0261335" rpy="0 0 0"/>
        <mass value="0.0230126"/>
        <inertia
          ixx="6.0949E-06" ixy="6.0665E-11" ixz="7.1910E-11"
          iyy="6.0196E-06" iyz="-2.7532E-06" izz="5.0786E-06"/>
      </inertial>

      <visual>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/left_inner_knuckle.stl"/>
        </geometry>
      </visual>

      <collision>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/left_inner_knuckle.stl"/>
        </geometry>
      </collision>
    </link>

    <joint name="${prefix}left_inner_knuckle_joint" type="revolute">
      <origin xyz="0 0.02 0.074098" rpy="0 0 0"/>
      <parent link="${prefix}xarm_gripper_base_link"/>
      <child link="${prefix}left_inner_knuckle"/>
      <axis xyz="1 0 0"/>
      <limit lower="0" upper="0.85" effort="50" velocity="2"/>
    </joint>

    <!--RIGHT OUTER KNUCKLE-->
    <link name="${prefix}right_outer_knuckle">
      <inertial>
        <origin xyz="-3.1669E-14 -0.021559 0.015181" rpy="0 0 0"/>
        <mass value="0.033618"/>
        <inertia
          ixx="1.9111E-05" ixy="-1.8789E-17" ixz="1.0986E-17"
          iyy="6.6256E-06" iyz="7.3008E-06" izz="1.3185E-05"/>
      </inertial>

      <visual>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/right_outer_knuckle.stl"/>
        </geometry>
      </visual>

      <collision>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/right_outer_knuckle.stl"/>
        </geometry>
      </collision>
    </link>

    <joint name="${prefix}right_outer_knuckle_joint" type="revolute">
      <origin xyz="0 -0.035 0.059098" rpy="0 0 0"/>
      <parent link="${prefix}xarm_gripper_base_link"/>
      <child link="${prefix}right_outer_knuckle"/>
      <axis xyz="-1 0 0"/>
      <limit lower="0" upper="0.85" effort="50" velocity="2"/>
    </joint>

    <!--RIGHT FINGER-->
    <link name="${prefix}right_finger">
      <inertial>
        <origin xyz="2.5618E-14 0.016413 0.029258" rpy="0 0 0"/>
        <mass value="0.048304"/>
        <inertia
          ixx="1.7493E-05" ixy="-5.0014E-19" ixz="-7.5079E-18"
          iyy="1.7225E-05" iyz="-4.6435E-06" izz="5.1466E-06"/>
      </inertial>

      <visual>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/right_finger.stl"/>
        </geometry>
      </visual>

      <collision>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/right_finger.stl"/>
        </geometry>
      </collision>
    </link>

    <joint name="${prefix}right_finger_joint" type="revolute">
      <origin xyz="0 -0.035465 0.042039" rpy="0 0 0"/>
      <parent link="${prefix}right_outer_knuckle"/>
      <child link="${prefix}right_finger"/>
      <axis xyz="1 0 0"/>
      <limit lower="0" upper="0.85" effort="50" velocity="2"/>
    </joint>

    <!--RIGHT INNER KNUCKLE-->
    <link name="${prefix}right_inner_knuckle">
      <inertial>
        <origin xyz="1.866E-06 -0.022047 0.026133" rpy="0 0 0"/>
        <mass value="0.023013"/>
        <inertia
          ixx="6.0949E-06" ixy="-6.0665E-11" ixz="7.191E-11"
          iyy="6.0197E-06" iyz="2.7531E-06" izz="5.0784E-06"/>
      </inertial>

      <visual>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/right_inner_knuckle.stl"/>
        </geometry>
      </visual>

      <collision>
        <geometry>
          <mesh filename="package://pickplace/meshes/gripper/xarm/right_inner_knuckle.stl"/>
        </geometry>
      </collision>
    </link>

    <joint name="${prefix}right_inner_knuckle_joint" type="revolute">
      <origin xyz="0 -0.02 0.074098" rpy="0 0 0"/>
      <parent link="${prefix}xarm_gripper_base_link"/>
      <child link="${prefix}right_inner_knuckle"/>
      <axis xyz="-1 0 0"/>
      <limit lower="0" upper="0.85" effort="50" velocity="2"/>
    </joint>

    <!--TCP LINK-->
    <link name="${prefix}link_tcp"/>

    <joint name="${prefix}joint_tcp" type="fixed">
      <origin xyz="0 0 0.172" rpy="0 0 0"/>
      <parent link="${prefix}xarm_gripper_base_link"/>
      <child link="${prefix}link_tcp"/>
    </joint>

    <!--TRANSMISSION-->
    <transmission name="${prefix}gripper_transmission">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${prefix}drive_joint">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="${prefix}gripper_motor">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

  </xacro:macro>
</robot>
